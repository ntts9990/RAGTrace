# RAGTrace Environment Configuration Example
# Copy this file to .env and update with your actual API keys

# Required: Google Gemini API Key
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Naver CLOVA Studio API Key (for HCX models)
# Get your key from: https://www.ncloud.com/product/aiService/clovaStudio
CLOVA_STUDIO_API_KEY=your_clova_studio_api_key_here

# Model Configuration (Optional - defaults provided)
GEMINI_MODEL_NAME=models/gemini-2.5-flash-preview-05-20
GEMINI_EMBEDDING_MODEL_NAME=models/gemini-embedding-exp-03-07
HCX_MODEL_NAME=HCX-005

# Default LLM Provider (gemini or hcx)
DEFAULT_LLM=gemini

# Database Configuration (Optional - for PostgreSQL)
POSTGRES_PASSWORD=ragtrace_password

# Streamlit Configuration (Optional - for web dashboard)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false